import java.awt.*;

public class MyImage extends MyDrawing {
	public MyImage() {

	}

	public void draw(Graphics g) {
		super.draw(g);

		int x = getX();
		int y = getY();
		int w = getW();
		int h = getH();

		// 高さや横幅が負の時の為の処理
		if (w < 0) {
			x += w;
			w *= -1;
		} if (h < 0 ) {
			y += h;
			h *= -1;
		}
	}
}
